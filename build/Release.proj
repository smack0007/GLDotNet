<Project DefaultTargets="Release">
  <Import Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props', '$(MSBuildThisFileDirectory)../'))" />

  <Target Name="Release" DependsOnTargets="DebugInfo;PushNuGet" />

  <Target Name="DebugInfo">
    <Message Importance="High" Text="ArtifactStagingDirectory = '$(ArtifactStagingDirectory)'" />
  </Target>

  <Target Name="PushNuGet">
  </Target>
  
</Project>
